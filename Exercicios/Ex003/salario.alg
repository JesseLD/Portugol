Algoritmo "salario"

Var nomeDofuncionario:caractere
    salarioAtual:real


Inicio
    escreval("Opa")
    escreval("Nome: ")
    leia(nomeDofuncionario)
    escreval("Salario: ")
    leia(salarioAtual)
    escreval("O funcionário ", nomeDofuncionario ," tem um salário atual de",salarioAtual, " Dólares")

Fimalgoritmo